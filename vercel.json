{
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/:path((?!offline\\.html$).*)",
      "has": [
        {
          "type": "header",
          "key": "Sec-Fetch-Dest",
          "value": "document"
        }
      ],
      "missing": [
        {
          "type": "header",
          "key": "Service-Worker",
          "value": "script"
        }
      ],
      "destination": "/offline.html"
    }
  ]
}
